{
  "name": "linkerr",
  "version": "1.2.0",
  "description": "Simple site parser",
  "type": "module",
  "exports": "./dist/linkerr.js",
  "types": "./dist/linkerr.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "ava",
    "build": "tsc",
    "prebuild": "rimraf dist",
    "prepublishOnly": "npm run build",
    "prepare": "husky install"
  },
  "author": {
    "name": "Vitalij Ryndin",
    "email": "sys@crashmax.ru",
    "url": "https://crashmax.ru"
  },
  "homepage": "https://github.com/crashmax-dev/linkerr",
  "repository": {
    "type": "git",
    "url": "https://github.com/crashmax-dev/linkerr.git"
  },
  "license": "MIT",
  "keywords": [
    "parse",
    "url",
    "link"
  ],
  "devDependencies": {
    "@sindresorhus/tsconfig": "^2.0.0",
    "@types/node": "^16.11.1",
    "ava": "^3.0.0",
    "husky": "^7.0.4",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "got": "^11.8.2",
    "node-html-parser": "^5.0.0"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nonSemVerExperiments": {
      "configurableModuleFormat": true
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  }
}
